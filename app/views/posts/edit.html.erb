<h1>Editing Post</h1>

<%= form_for @post do |f| %>
  <div class="login">

    <div class="field">
      <%= f.label :title %>
      <%= f.text_field :title %>
    </div>
    <div class="actions"><%= f.submit %></div>
    </div>
  <% end %>
<div class="login">
<%= link_to 'Cancel', posts_path %>
</div>